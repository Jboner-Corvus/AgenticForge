{
  "name": "agentic-forge-server",
  "version": "1.0.0",
  "description": "AgenticForge Server",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "REDIS_HOST=localhost REDIS_PORT=6378 node dist/server.js",
    "dev": "NODE_ENV=development nodemon --watch src --exec node --loader ts-node/esm src/server.ts",
    "build": "tsup src/server.ts src/worker.ts src/tools/**/*.ts --format esm --dts --clean",
    "build:no-dts": "tsup src/server.ts src/worker.ts src/tools/**/*.ts --format esm --clean",
    "start:worker": "node dist/worker.js",
    "test": "vitest run",
    "lint": "eslint .",
    "format": "prettier --write . && eslint --fix ."
  },
  "dependencies": {
    "bullmq": "^5.8.2",
    "cookie-parser": "^1.4.6",
    "dockerode": "^4.0.2",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "fastmcp": "^3.7.0",
    "ioredis": "^5.4.1",
    "pino": "^9.1.0",
    "playwright": "^1.44.1",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@modelcontextprotocol/inspector": "^0.15.0",
    "@sebbo2002/semantic-release-jsr": "^3.0.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/node": "^24.0.7",
    "@types/uri-templates": "^0.1.34",
    "@types/yargs": "^17.0.33",
    "@valibot/to-json-schema": "^1.3.0",
    "@wong2/mcp-cli": "^1.10.0",
    "arktype": "^2.1.20",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-perfectionist": "^4.15.0",
    "eslint-plugin-prettier": "^5.5.1",
    "eventsource-client": "^1.1.3",
    "get-port-please": "^3.1.2",
    "jiti": "^2.4.2",
    "jsr": "^0.13.4",
    "pino-pretty": "^13.0.0",
    "prettier": "^3.6.2",
    "semantic-release": "^24.2.6",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0",
    "valibot": "^1.1.0",
    "vitest": "^3.2.4"
  }
}